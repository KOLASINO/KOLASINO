terraform {
  required_providers {
    azurerm = {
      source  = "hashicorp/azurerm"
      version = "3.85.0"
    }
  }
}

provider "azurerm" {
  features {
  }
resource "azurerm_resource_group" "resourcegroup" {
  name     = "rg11"
  location = "West Europe"
}
